@model PortalDeTraducoes.Models.Entities.Publisher


<div id="PublisherLogo">
    <img src="@Model.ImageUrl" />
</div>
<div id="PublisherName">
    <h2>@Model.Name</h2>
</div>
<h3>Games:</h3>
<div>
    <ul>

        @foreach (var game in Model.Games)
        {
            <li><a asp-controller="Games" asp-action="Game" asp-route-title="@game.Title">@game.Title</a> </li>
        }
    </ul>
</div>
